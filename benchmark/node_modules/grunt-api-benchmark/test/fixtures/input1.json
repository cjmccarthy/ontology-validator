{
  "service": {
    "My api": "http://localhost:8888"
  },
  "endpoints": {
    "postOntology": {
      "route": "/api/ontology/update",
      "method": "post",
      "data": {
        "animal": {
          "*weight": "?",
          "domestic": {
            "*name": "+",
            "cat": {
              "*declawed": "1",
              "declawed": "boolean"
            },
            "dog": {
              "*trained": "1",
              "breed": "string",
              "trained": "boolean"
            },
            "name": "string"
          },
          "livestock": {
            "*value": "?",
            "cattle": {
              "*angus": "?",
              "*grassFed": "?",
              "angus": "boolean",
              "grassFed": "boolean"
            },
            "chicken": {
              "*eggLaying": "1",
              "eggLaying": "boolean",
              "eggsProduced": "int"
            },
            "value": "float"
          },
          "weight": "int"
        }
      }
    },
    "fetchOntology": "/api/ontology/fetch", 
    "validateOntology": {
      "route": "/api/ontology/validate",
      "method": "post",
      "data": {
        "eggLaying": false,
        "type": "animal.livestock.chicken",
        "value": 13.37,
        "weight": 10
      }
    },
    "validateBadOntology": {
      "route": "/api/ontology/validate",
      "method": "post",
      "data": {
        "eggLaying": 25,
        "type": "anil.liveock.chicken",
        "value": 13.37,
        "weight": 10
      }
    }
  },
  "options": {
    "minSamples": 5000,
    "runMode": "parallel",
    "maxConcurrentRequests": 5,
    "debug": true
  }
}
